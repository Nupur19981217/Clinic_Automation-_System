                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @if (Request.IsAuthenticated && Session["EmailId"] != null)
            {
                @Html.ActionLink("Logout", "Logout", "Home", new { area = "" }, new { @class = "navbar-brand" })            
                @Html.ActionLink("About Us", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Contact", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            }
            else
            {
                @Html.ActionLink("Login", "Login", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Register", "Register", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("About Us", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Contact", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            }
        </div>
        <div class="navbar-collapse collapse">
