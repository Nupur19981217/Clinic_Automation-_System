@model ClinicalAutomationSystem_MVC_.Models.PatientModel
@{
    ViewBag.Title = "EditProfile";
}

<h2>EditProfile</h2>

@using (Html.BeginForm("EditProfile", "Patient"))
{
    <div>



        First Name:@Html.TextBoxFor(m => m.FName)
        <br />
        <br />
        Last Name:@Html.TextBoxFor(m => m.LName)
        <br />
        <br />
        Date Of Birth :@Html.TextBoxFor(m=>m.DOB)
        <br />
        <br />
        Address:@Html.TextBoxFor(m => m.Address)

        <br />
        <br />
        Gender:Male-@Html.RadioButtonFor(m => m.Gender, "male")
        Female-@Html.RadioButtonFor(m => m.Gender, "female")
        <br />
        <br />

        <input type="submit" value="submit" />

        <br />
        @ViewBag.text
    </div>
}